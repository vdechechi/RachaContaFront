info = response.data; // Extract data from response\n      })\n      .catch(error => {\n        console.error('Error fetching event data:', error);\n      });\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.container {\n  display: flex;\n  flex-direction: column;\n  align-items: center; /* Center content horizontally */\n  justify-content: center; /* Center content vertically */\n  height: 100vh; /* Full viewport height */\n}\n\n.dx-card {\n  background-color: #535353;\n  display: flex;\n  flex-direction: column;\n  align-items: center; /* Center buttons inside card */\n  margin-bottom: 20px; /* Spacing between cards */\n}\n\n.buttons-events {\n  margin-bottom: 10px;\n}\n\n.app{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>\n"],"mappings":"AA+DA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,SAAQ,MAAO,yBAAyB;AAC/C,SAASC,OAAO,CAAE,yBAAyB,sBAAsB;AACjE,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,WAAU,QAAS,2BAA2B;AAEvD,eAAe;EACbC,IAAI,EAAE,YAAY;EAAE;;EAEpBC,UAAU,EAAE;IACVN,QAAQ;IACRE,OAAO;IACPD,SAAS;IACTG;IACA;EACF,CAAC;EAEDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,QAAQ,EAAE,OAAO,EAAE,UAAU,EAAE,QAAQ,CAAC;MACjDC,YAAY,EAAE,KAAK;MACnBC,IAAI,EAAE,IAAI;MACVC,kBAAkB,EAAE;QAClB;MAAA;IAEJ,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACJ,YAAW,GAAI,IAAI;IAC1B,CAAC;IAEDK,UAAUA,CAAA,EAAG;MACX,IAAI,CAACL,YAAW,GAAI,KAAK;IAC3B;EACF,CAAC;EAEDM,OAAOA,CAAA,EAAG;IACRZ,KAAI,CACDa,GAAG,CAAC,uCAAuC,EAC3CC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACR,IAAG,GAAIQ,QAAQ,CAACX,IAAI,EAAE;IAC7B,CAAC,EACAY,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;IACpD,CAAC,CAAC;EACN;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}