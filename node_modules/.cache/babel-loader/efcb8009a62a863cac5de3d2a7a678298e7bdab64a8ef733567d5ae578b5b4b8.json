Trabalho\\racha-conta\\front\\src\\components\\create-event.vue"],"sourcesContent":["<template>\r\n  <DxPopup\r\n      :visible=\"localPopupVisible\"\r\n      :drag-enabled=\"false\"\r\n      :hide-on-outside-click=\"true\"\r\n      :show-close-button=\"true\"\r\n      :width=\"500\"\r\n      :height=\"600\"\r\n      @hidden=\"closePopUp\"\r\n      :close-button-options=\"closeButtonOptions\"\r\n      title=\"Criar Evento\"\r\n    >\r\n      <DxTextBox\r\n        v-model=\"eventName\"\r\n        class=\"event_name\"\r\n        :input-attr=\"{ 'aria-label': 'Name' }\"\r\n        placeholder=\"Nome do Evento\"\r\n      />\r\n\r\n      <div class=\"infos\">\r\n        <div class=\"pessoas\">\r\n          <span>Pessoas</span>\r\n          <DxNumberBox\r\n            v-model=\"peopleCount\"\r\n            :max=\"30\"\r\n            :min=\"1\"\r\n            :show-spin-buttons=\"true\"\r\n            :input-attr=\"{ 'aria-label': 'People Count' }\"\r\n          />\r\n        </div>\r\n\r\n        <div class=\"dias\">\r\n          <span>Dias</span>\r\n          <DxNumberBox\r\n            v-model=\"daysCount\"\r\n            :max=\"30\"\r\n            :min=\"1\"\r\n            :show-spin-buttons=\"true\"\r\n            :input-attr=\"{ 'aria-label': 'Days Count' }\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"infos\">\r\n        <DxTextBox\r\n          v-model=\"purchaseTitle\"\r\n          class=\"title_compra\"\r\n          :input-attr=\"{ 'aria-label': 'Purchase Title' }\"\r\n          placeholder=\"Compra\"\r\n        />\r\n\r\n        <div class=\"value\">\r\n          <span>Valor em R$:</span>\r\n          <DxNumberBox\r\n            v-model=\"valueBRL\"\r\n            :max=\"100000\"\r\n            :min=\"0\"\r\n            :show-spin-buttons=\"true\"\r\n            :input-attr=\"{ 'aria-label': 'Value in BRL' }\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <DxButton\r\n        text=\"Salvar Evento\"\r\n        type=\"success\"\r\n        styling-mode=\"contained\"\r\n        @click=\"criarEvento\"\r\n      />\r\n    </DxPopup>\r\n</template>\r\n\r\n<script>\r\nimport DxTextBox from 'devextreme-vue/text-box';\r\nimport { DxPopup } from 'devextreme-vue/popup';\r\nimport { DxNumberBox } from 'devextreme-vue/number-box';\r\nimport DxButton from 'devextreme-vue/button';\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: \"CreateEvent\",\r\n\r\n  props: {\r\n    popupVisible: Boolean\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      localPopupVisible: this.popupVisible, // variável local para o controle do popup\r\n      eventName: '',\r\n      peopleCount: 1,\r\n      daysCount: 1,\r\n      purchaseTitle: '',\r\n      valueBRL: 0\r\n    };\r\n  },\r\n\r\n  watch: {\r\n    popupVisible(newValue) {\r\n      this.localPopupVisible = newValue;\r\n    }\r\n  },\r\n\r\n  components: {\r\n    DxPopup,\r\n    DxNumberBox,\r\n    DxTextBox,\r\n    DxButton\r\n  },\r\n\r\n  methods: {\r\n    // Método para criar o evento e enviar a requisição POST\r\n    criarEvento() {\r\n      const novoEvento = {\r\n        nome: this.eventName,\r\n        pessoas: this.peopleCount,\r\n        dias: this.daysCount,\r\n        compra: this.purchaseTitle,\r\n        valor: this.valueBRL\r\n      };\r\n\r\n      axios.post('http://localhost:3000/criar_evento', novoEvento)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          alert('Evento criado com sucesso!');\r\n          this.closePopUp(); // Fecha o popup após o sucesso\r\n        })\r\n        .catch(error => {\r\n          console.error('Ocorreu um erro ao criar o evento:', error);\r\n        });\r\n    },\r\n    closePopUp() {\r\n      this.localPopupVisible = false;\r\n      this.$emit(\"update:popupVisible\", false);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"mappings":"AAyEA,OAAOA,SAAQ,MAAO,yBAAyB;AAC/C,SAASC,OAAM,QAAS,sBAAsB;AAC9C,SAASC,WAAU,QAAS,2BAA2B;AACvD,OAAOC,QAAO,MAA